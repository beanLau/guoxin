if !reportDetail
    div(style="margin:40px auto;color:#999;font-size:26px;text-align:center;") 暂无数据
else
    #detal_sub_content
        p(style="text-align:center;font-weight:bold;") 钟表维修保卡报告单
        table(width="100%",border="0",cellpadding="0",cellspacing="1").table.table-bordered.table-reportdetail.detail.detail
            tr
                td.table-label 系统识别号
                td(colspan=3) !{reportDetail.shibieCode || ""}
                td.table-label 维修时间
                td(colspan=3) !{reportDetail.repairEndDate || ""}
            tr
                td.table-label 品牌名称
                td(colspan=3) !{reportDetail.brandName || ""}
                td.table-label 表壳号
                td(colspan=3) !{reportDetail.bkeCode || ""}
            tr
                td.table-label 表壳序号
                td !{reportDetail.bkeOrder || ""}
                td.table-label 机芯型号
                td !{reportDetail.jixinStyle || ""}
                td.table-label 机芯序号
                td !{reportDetail.jxorder || ""}