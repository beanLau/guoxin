.container.page-content
    .row
        .col-xs-12
            img(src="../../images/adverte_course.jpg").middle-pic
    .row.mg20
        .col-sm-3.col-sx-12.gx-btn-list
            div
                #main_left_title 报告查询
                #main_left_menu
                    ul
                        if (!reportType || reportType == 0)
                            li.nei_menu_on
                                a(href="#",title="钟表型式检验报告查询") 钟表型式检验报告查询
                        else
                            li.nei_menu_off
                                a(href="/report",title="钟表型式检验报告查询") 钟表型式检验报告查询
                        if  reportType == 1
                            li.nei_menu_on
                                a(href="#",title="司法技术鉴定报告查询") 司法技术鉴定报告查询
                        else
                            li.nei_menu_off
                                a(href="/report?type=1",title="司法技术鉴定报告查询") 司法技术鉴定报告查询
                        if  reportType == 2
                            li.nei_menu_on
                                a(href="#",title="二手表检验证书查询") 二手表检验证书查询
                        else
                            li.nei_menu_off
